{
  "rules": {
    "teams": { 
        ".read": "root.child('users').child(auth.uid).child('active').val() === true",
        ".write": "root.child('users').child(auth.uid).child('active').val() === true"
    },
    "users": {
      	"$uid": {
          ".read": "true",
          ".write": "(!newData.hasChild('active')) || (root.child('admins').child(auth.uid).val() === true)"
        },
				".read": "true"
    },
    "admins": {
        ".read": "true",
        ".write": "false"
    }
  }
}
